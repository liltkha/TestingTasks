{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "class-methods-use-this": "off",
        "arrow-parens": "off",
        "prefer-arrow-callback": "off",
        "func-names": "off",
        "object-curly-newline": "off",
        "FunctionDeclaration": true,
                "MethodDefinition": false,
                "ClassDeclaration": false,
                "ArrowFunctionExpression": false,
                "FunctionExpression": false
    }
}
